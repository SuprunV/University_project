
@{
}
@foreach(string checkboxName in Model.CheckBoxFilters) {
    var checkedVal = !string.IsNullOrEmpty(Model?.UserID ?? string.Empty) ?? false;        
<div class="col-md-3 form-check form-switch">
  <input name="@checkboxName" class="form-check-input" type="checkbox" id="flexSwitchCheckDefault" @(checkedVal ? "checked" : "")>
  <label class="form-check-label" for="flexSwitchCheckDefault">@Model.DisplayName(checkboxName) (@Model.mandatoryStudyProgram)</label>
</div>
}
